Private Sub TDBText1_Change(Index As Integer)
Dim tmp As String, tmp1 As String, j As Integer, i As Integer

On Error GoTo errhandler

'check if there is any date
If ToggleButton1(0).Value = True Then
    If TDBCombo2(0).Text = "исо ле:" Then
        If TDBDate1(0).ValueIsNull = True Then
            MsgBox "паяайакы сулпкгяысте тгм глеяолгмиа.", vbApplicationModal + vbOKOnly, "йемтяийа аявеиа"
            ToggleButton1(0).Value = False
            Exit Sub
        End If
    End If
    If TDBCombo2(0).Text = "летану:" Then
        If TDBDate1(0).ValueIsNull = True Or TDBDate1(10).ValueIsNull = True Then
            MsgBox "паяайакы сулпкгяысте йаи тис дуо глеяолгмиес.", vbApplicationModal + vbOKOnly, "йемтяийа аявеиа"
            ToggleButton1(0).Value = False
            Exit Sub
        End If
    End If
End If
    

'search for string values
cnt1 = 0
For i = TDBText1.LBound To TDBText1.UBound
If Trim(TDBText1(i).Text) <> "" Then
    cnt1 = cnt1 + 1
    j = TDBCombo1(i).SelectedItem
    If cnt1 > 1 Then
        tmp = tmp & " AND "
    End If

    Select Case j
        Case 2
            If i < 2 Then
                tmp = tmp & mainMDIForm1.TData19.Fields(i).DataSourceField & " LIKE '" & TDBText1(i).Text & "*'"
            Else
                tmp = tmp & mainMDIForm1.TData19.Fields(4).DataSourceField & " LIKE '" & TDBText1(i).Text & "*'"
            End If
        Case 1
            If i < 2 Then
                tmp = tmp & mainMDIForm1.TData19.Fields(i).DataSourceField & " LIKE '*" & TDBText1(i).Text & "*'"
            Else
                tmp = tmp & mainMDIForm1.TData19.Fields(4).DataSourceField & " LIKE '*" & TDBText1(i).Text & "*'"
            End If
    End Select

End If
Next i

'search for numeric and date values
If tmp <> "" Then
    ima = False
    For i = ToggleButton1.LBound To ToggleButton1.UBound
        If ToggleButton1(i).Value = True Then ima = True: Exit For
    Next i
    If ima = True Then
        tmp1 = tmp & " AND ("
    Else
        tmp1 = tmp
        mainMDIForm1.TData19.SourceRecordset.Filter = tmp1
        DoEvents
        Exit Sub
    End If
End If

cnt1 = 0
For i = ToggleButton1.LBound To ToggleButton1.UBound

If ToggleButton1(i).Value = True Then
    cnt1 = cnt1 + 1
    j = TDBCombo2(i).SelectedItem
    If cnt1 > 1 Then tmp1 = tmp1 & " and "
Select Case j
    Case 1
        If i = 0 Then
            tmp1 = tmp1 & mainMDIForm1.TData19.Fields(i + 2).Name & " =#" & TDBDate1(i).Value & "#"
        Else
            tmp1 = tmp1 & mainMDIForm1.TData19.Fields(i + 2).Name & " ='" & TDBNumber1(i).Value & "'"
        End If
    Case 2
        If i = 0 Then
        tmp1 = tmp1 & mainMDIForm1.TData19.Fields(i + 2).Name & " >#" & TDBDate1(i).Value & "#"
        Else
        tmp1 = tmp1 & mainMDIForm1.TData19.Fields(i + 2).Name & " >'" & TDBNumber1(i).Value & "'"
        End If
    Case 3
        If i = 0 Then
            tmp1 = tmp1 & mainMDIForm1.TData19.Fields(i + 2).Name & " <#" & TDBNumber1(i).Value & "#"
        Else
            tmp1 = tmp1 & mainMDIForm1.TData19.Fields(i + 2).Name & " <'" & TDBNumber1(i).Value & "'"
        End If
    Case 4
        If i = 0 Then
        tmp1 = tmp1 & mainMDIForm1.TData19.Fields(i + 2).Name & " >= #" & TDBDate1(i).Value & "#" & _
        " and " & mainMDIForm1.TData19.Fields(i + 2).Name & " <= #" & TDBDate1(i + 10).Value & "#"
        Else
        tmp1 = tmp1 & mainMDIForm1.TData19.Fields(i + 2).Name & " >= '" & TDBNumber1(i).Value & "'" & _
        " and " & mainMDIForm1.TData19.Fields(i + 2).Name & " <= '" & TDBNumber1(i + 10).Value & "'"
        End If
End Select

End If
Next i

If tmp <> "" Then tmp1 = tmp1 & " )"
mainMDIForm1.TData19.SourceRecordset.Filter = tmp1
DoEvents

errhandler:
If Err.Number = 0 Then Exit Sub
MsgBox Err.Number & ":" & " " & Err.Source & ":" & " " & Err.Description, vbApplicationModal + vbOKOnly, "йемтяийа аявеиа"

End Sub

