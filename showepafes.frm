Private Sub Form_Load()
Dim i As Integer

Me.Caption = "амакутийа стоивеиа тоу:" & epafes.fg1.TextMatrix(epafes.fg1.Row, 2)

For i = TDBText1.lBound To TDBText1.UBound
TDBText1(i).Key.NextCtrl = "{Down}"
TDBText1(i).Key.PrevCtrl = "{Up}"
Next i

For i = 2 To epafes.fg1.Cols - 1
TDBText1(i - 2).Text = epafes.fg1.TextMatrix(epafes.fg1.Row, i)
Next i
End Sub
