Private Sub TDBCombo4_SelChange(Index As Integer, Cancel As Integer)
Dim tmp As String, j As Integer, i As Integer

On Error GoTo errhandler


cnt1 = 0
For i = TDBText1.LBound To TDBText1.UBound
If Trim(TDBText1(i).Text) <> "" Then
    cnt1 = cnt1 + 1
    j = TDBCombo4(i).SelectedItem
    If cnt1 > 1 Then
        tmp = tmp & " AND "
    End If
    
    Select Case j
        Case 2
         tmp = tmp & mainMDIForm1.TData1.Fields(i + 1).DataSourceField & " LIKE '" & TDBText1(i).Text & "*'"
        Case 1
        tmp = tmp & mainMDIForm1.TData1.Fields(i + 1).DataSourceField & " LIKE '*" & TDBText1(i).Text & "*'"
