Private Sub TDBText1_Change(Index As Integer)
Dim tmp As String, tmp1 As String, j As Integer, i As Integer

On Error GoTo errhandler

'search for string values
cnt1 = 0
For i = TDBText1.LBound To TDBText1.UBound
If Trim(TDBText1(i).Text) <> "" Then
    cnt1 = cnt1 + 1
    j = TDBCombo1(i).SelectedItem
    If cnt1 > 1 Then
        tmp = tmp & " AND "
    End If

    Select Case j
        Case 2
        tmp = tmp & mainMDIForm1.TData12.Fields(i).DataSourceField & " LIKE '" & TDBText1(i).Text & "*'"
        Case 1
        tmp = tmp & mainMDIForm1.TData12.Fields(i).DataSourceField & " LIKE '*" & TDBText1(i).Text & "*'"
    End Select

End If
Next i

'search for numeric values
If tmp <> "" Then
    ima = False
    
        If ToggleButton1.Value = True Then ima = True
    
    If ima = True Then
        tmp1 = tmp & " AND ("
    Else
        tmp1 = tmp
        mainMDIForm1.TData12.SourceRecordset.Filter = tmp1
        DoEvents
        Exit Sub
    End If
End If

cnt1 = 0


If ToggleButton1.Value = True Then
    cnt1 = cnt1 + 1
    j = TDBCombo2.SelectedItem
    If cnt1 > 1 Then tmp1 = tmp1 & " and "
Select Case j
    Case 1
        tmp1 = tmp1 & mainMDIForm1.TData12.Fields(4).Name & " ='" & TDBNumber1(0).Value & "'"
    Case 2
        tmp1 = tmp1 & mainMDIForm1.TData12.Fields(4).Name & " >'" & TDBNumber1(0).Value & "'"
    Case 3
        tmp1 = tmp1 & mainMDIForm1.TData12.Fields(4).Name & " <'" & TDBNumber1(0).Value & "'"
    Case 4
     tmp1 = tmp1 & mainMDIForm1.TData12.Fields(4).Name & " >= '" & TDBNumber1(0).Value & "'" & _
        " and " & mainMDIForm1.TData12.Fields(4).Name & " <= '" & TDBNumber1(1).Value & "'"
End Select

End If

If tmp <> "" Then tmp1 = tmp1 & " )"
mainMDIForm1.TData12.SourceRecordset.Filter = tmp1
DoEvents

errhandler:
If Err.Number = 0 Then Exit Sub
MsgBox Err.Number & ":" & " " & Err.Source & ":" & " " & Err.Description, vbApplicationModal + vbOKOnly, "йемтяийа аявеиа"

End Sub

